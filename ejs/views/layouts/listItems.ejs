<h1> * Productos * </h1>

<% if(lenItems){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Foto</th>
            </tr>
            <%items.forEach(function(item) {%>
                <tr>
                    <td>
                        <%=item.title%>
                    </td>
                    <td>
                        <%=item.price%>
                    </td>
                    <td><img width="50" src="<%=item.thumb%>" alt="not found"></td>
                </tr>
                <%});%>
        </table>
    </div>
    <% } else{ %>
        <h3 class="alert alert-warning">No se encontraron productos</h3>
    <% } %>

    <br>
    <a href="/" type="button" class="btn btn-primary">Cargar un nuevo producto</a>
 